<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ConstraintLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/image2"
    tools:context=".Assignment_2.Student_Activity_One">


    <Button
        android:id="@+id/back"
        android:layout_width="73dp"
        android:layout_height="66dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="31dp"
        android:background="@drawable/baseline_arrow_back_ios_24"
        android:contentDescription="@string/hello_first_fragment"
        android:textColor="@color/white"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.065"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="137dp"
        android:layout_height="104dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <Button
            android:id="@+id/logo"
            android:layout_width="105dp"
            android:layout_height="99dp"
            android:background="@drawable/baseline_check_circle_outline_24"
            android:contentDescription="@string/hello_first_fragment"
            android:textColor="@color/white"
            android:textSize="2dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.571"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="382dp"
        android:layout_height="478dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toTopOf="@+id/save"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints">

        <EditText
            android:id="@+id/Student_name"
            android:layout_width="360dp"
            android:layout_height="69dp"
            android:autofillHints="Name"
            android:background="@drawable/rounded_corner_backgound_2"
            android:drawableStart="@drawable/baseline_person_24"
            android:drawableTint="#A8000000"
            android:ems="12"
            android:hint="    Enter Name"
            android:inputType="textPersonName"
            android:padding="12dp"
            android:paddingStart="32dp"
            android:shadowColor="#000000"
            android:textColor="#A8000000"
            android:textColorHighlight="#A8000000"
            android:textColorHint="#A8000000"
            android:textSize="22dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.059" />

        <EditText
            android:id="@+id/Student_cnic"
            android:layout_width="360dp"
            android:layout_height="75dp"
            android:autofillHints="CNIC"
            android:background="@drawable/rounded_corner_backgound_2"
            android:drawableStart="@drawable/baseline_credit_card_24"
            android:drawableTint="#A8000000"
            android:ems="12"
            android:hint="    Enter CNIC"
            android:inputType="text|number"
            android:padding="12dp"
            android:paddingStart="32dp"
            android:shadowColor="#000000"
            android:textColor="#A8000000"
            android:textColorHighlight="#A8000000"
            android:textColorHint="#A8000000"
            android:textSize="22dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Student_name"
            app:layout_constraintVertical_bias="0.059"
            tools:ignore="NotSibling" />

        <EditText
            android:id="@+id/Student_phone"
            android:layout_width="360dp"
            android:layout_height="75dp"
            android:autofillHints="Phone"
            android:background="@drawable/rounded_corner_backgound_2"
            android:drawableStart="@drawable/baseline_local_phone_24"
            android:drawableTint="#A8000000"
            android:ems="12"
            android:hint="    Enter Phone"
            android:inputType="text|phone"
            android:padding="12dp"
            android:paddingStart="32dp"
            android:shadowColor="#000000"
            android:textColor="#A8000000"
            android:textColorHighlight="#A8000000"
            android:textColorHint="#A8000000"
            android:textSize="22dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Student_cnic"
            app:layout_constraintVertical_bias="0.084"
            tools:ignore="NotSibling" />

        <EditText
            android:id="@+id/Student_SGPA"
            android:layout_width="360dp"
            android:layout_height="75dp"
            android:autofillHints="SGPA"
            android:background="@drawable/rounded_corner_backgound_2"
            android:drawableStart="@drawable/baseline_format_list_numbered_24"
            android:drawableTint="#A8000000"
            android:ems="12"
            android:hint="    Enter SGPA"
            android:inputType="text|numberDecimal"
            android:padding="12dp"
            android:paddingStart="32dp"
            android:shadowColor="#000000"
            android:textColor="#A8000000"
            android:textColorHighlight="#A8000000"
            android:textColorHint="#A8000000"
            android:textSize="22dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Student_phone"
            app:layout_constraintVertical_bias="0.137" />

        <EditText
            android:id="@+id/Student_CGPA"
            android:layout_width="360dp"
            android:layout_height="75dp"
            android:autofillHints="CGPA"
            android:background="@drawable/rounded_corner_backgound_2"
            android:drawableStart="@drawable/baseline_format_list_numbered_rtl_24"
            android:drawableTint="#A8000000"
            android:ems="12"
            android:hint="    Enter CGPA"
            android:inputType="text|numberDecimal"
            android:padding="12dp"
            android:paddingStart="32dp"
            android:shadowColor="#000000"
            android:textColor="#A8000000"
            android:textColorHighlight="#A8000000"
            android:textColorHint="#A8000000"
            android:textSize="22dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.619"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Student_SGPA" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/save"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="36dp"
        android:background="#00000000"
        android:text="Save"
        android:textColor="@color/white"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="236dp"
        android:layout_height="70dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="17dp"
        android:fontFamily="@font/adamina"
        android:text="Database"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        app:layout_constraintVertical_bias="0.47"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>